{
  "name": "@unminify-kit/test-utils",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.6.3",
  "author": "Pionxzh",
  "license": "MIT",
  "sideEffects": false,
  "main": "src/index.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap --clean",
    "test:type": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "dependencies": {
    "jscodeshift": "^0.15.0",
    "vitest": "^0.34.4"
  },
  "devDependencies": {
    "@types/jscodeshift": "^0.11.6",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  }
}
