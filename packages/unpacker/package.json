{
  "name": "@wakaru/unpacker",
  "type": "module",
  "version": "0.1.2",
  "description": "ðŸ”ªðŸ“¦ Unminify and beautify bundled code",
  "author": "Pionxzh",
  "license": "MIT",
  "sideEffects": false,
  "main": "src/index.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin swc3",
    "test": "vitest run --globals",
    "test:update": "vitest run --update --globals",
    "test:watch": "vitest watch --globals",
    "test:type": "tsc --noEmit",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --fix --max-warnings=0"
  },
  "dependencies": {
    "@babel/helper-validator-identifier": "^7.24.7",
    "@babel/parser": "^7.25.0",
    "ast-types": "npm:ast-types-x@1.17.1",
    "jscodeshift": "npm:jscodeshift-x@1.2.0",
    "pathe": "^1.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@swc/core": "^1.7.2",
    "@types/jscodeshift": "^0.11.11",
    "@wakaru/ast-utils": "workspace:*",
    "@wakaru/shared": "workspace:*",
    "@wakaru/test-utils": "workspace:*",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-swc3": "^0.11.2",
    "typescript": "^5.5.4"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  }
}
